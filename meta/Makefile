codegenuse:
	time xsltproc --xinclude makeuse.xsl use.xml > ../Gtk4Sys/use.pony

codegenfunctions:
	cp functionheader.txt ../Gtk4Sys/functions.pony
	time xsltproc --xinclude makefunctions.xsl use.xml >> ../Gtk4Sys/functions.pony

functions:
	time ../../../castxml2pony/castxml2pony -x gtk4.xml ${GTKANDDEPS} -u
 
